{
  "version": 2,
  "framework": null,
  "buildCommand": "",
  "outputDirectory": ".",
  "installCommand": "echo 'No installation required'",
  "functions": {
    "api-functions/*.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api-functions/auth/*.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api-functions/health.js"
    },
    {
      "source": "/api/projects",
      "destination": "/api-functions/projects.js"
    },
    {
      "source": "/api/contacts",
      "destination": "/api-functions/contacts.js"
    },
    {
      "source": "/api/qualifications",
      "destination": "/api-functions/qualifications.js"
    },
    {
      "source": "/api/auth/signup",
      "destination": "/api-functions/auth/signup.js"
    },
    {
      "source": "/api/auth/signin",
      "destination": "/api-functions/auth/signin.js"
    },
    {
      "source": "/api/auth/signout",
      "destination": "/api-functions/auth/signout.js"
    },
    {
      "source": "/api/test",
      "destination": "/api-functions/test.js"
    },
    {
      "source": "/api/seed-data",
      "destination": "/api-functions/seed-data.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
