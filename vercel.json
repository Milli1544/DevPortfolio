{
  "version": 2,
  "framework": null,
  "buildCommand": "",
  "outputDirectory": ".",
  "installCommand": "echo 'No installation required'",
  "functions": {
    "api/health.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/projects.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/qualifications.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/contacts.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/test.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },

    "api/auth/signin.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/auth/signup.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/auth/signout.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/users.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/health.js"
    },
    {
      "source": "/api/projects",
      "destination": "/api/projects.js"
    },
    {
      "source": "/api/contacts",
      "destination": "/api/contacts.js"
    },
    {
      "source": "/api/qualifications",
      "destination": "/api/qualifications.js"
    },
    {
      "source": "/api/auth/signup",
      "destination": "/api/auth/signup.js"
    },
    {
      "source": "/api/auth/signin",
      "destination": "/api/auth/signin.js"
    },
    {
      "source": "/api/auth/signout",
      "destination": "/api/auth/signout.js"
    },
    {
      "source": "/api/users",
      "destination": "/api/users.js"
    },
    {
      "source": "/api/test",
      "destination": "/api/test.js"
    },

    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
