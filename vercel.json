{
  "version": 2,
  "framework": null,
  "functions": {
    "api-functions/*.js": {
      "runtime": "@vercel/node",
      "maxDuration": 30
    },
    "api-functions/auth/*.js": {
      "runtime": "@vercel/node",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api-functions/health.js"
    },
    {
      "src": "/api/projects",
      "dest": "/api-functions/projects.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/projects/(.*)",
      "dest": "/api-functions/projects.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/contacts",
      "dest": "/api-functions/contacts.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/contacts/(.*)",
      "dest": "/api-functions/contacts.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/qualifications",
      "dest": "/api-functions/qualifications.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/qualifications/(.*)",
      "dest": "/api-functions/qualifications.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/auth/signup",
      "dest": "/api-functions/auth/signup.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/auth/signin",
      "dest": "/api-functions/auth/signin.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/auth/signout",
      "dest": "/api-functions/auth/signout.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/test",
      "dest": "/api-functions/test.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": null,
  "outputDirectory": ".",
  "installCommand": null
}
