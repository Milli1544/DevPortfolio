{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "functions": {
    "api/health.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/projects.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/qualifications.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/contacts.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/test.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/env-test.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/simple-test.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/minimal-test.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },

    "api/auth/signin.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/auth/signup.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/auth/signout.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    },
    "api/users.js": {
      "runtime": "@vercel/node@2.15.9",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/health.js"
    },
    {
      "source": "/api/projects",
      "destination": "/api/projects.js"
    },
    {
      "source": "/api/contacts",
      "destination": "/api/contacts.js"
    },
    {
      "source": "/api/qualifications",
      "destination": "/api/qualifications.js"
    },
    {
      "source": "/api/auth/signup",
      "destination": "/api/auth/signup.js"
    },
    {
      "source": "/api/auth/signin",
      "destination": "/api/auth/signin.js"
    },
    {
      "source": "/api/auth/signout",
      "destination": "/api/auth/signout.js"
    },
    {
      "source": "/api/users",
      "destination": "/api/users.js"
    },
    {
      "source": "/api/test",
      "destination": "/api/test.js"
    },
    {
      "source": "/api/test-env",
      "destination": "/api/env-test.js"
    },
    {
      "source": "/api/test-simple",
      "destination": "/api/simple-test.js"
    },
    {
      "source": "/api/minimal-test",
      "destination": "/api/minimal-test.js"
    },

    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
